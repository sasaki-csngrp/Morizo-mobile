{
  "expo": {
    "name": "Morizo Mobile",
    "slug": "morizo-mobile",
    "scheme": "morizo-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Morizo_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash/Morizo_Splash_01.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash/Morizo_Splash_01.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "jp.co.csngrp.morizo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Morizo_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "jp.co.csngrp.morizo",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "056fa7c1-a535-45ca-863a-a01ac9a8b01c"
      }
    }
  }
}
